@model AcessoModel
@{
    ViewData["Title"] = "Confirmar exclusao";
}

<div class="text-center">

    <h3>Realmente deseja excluir o usuario (@Model.nome) ?</h3>

</div>

<a class="btn btn-primary btn-block" role="button" asp-controller="Login" asp-action="Index">Cancelar Exclusão </a>
<a class="btn btn-danger btn-block" role="button" asp-controller="Login" asp-action="ExcluirLogin" asp-route-id="@Model.id">Confirmar Exclusão </a>

