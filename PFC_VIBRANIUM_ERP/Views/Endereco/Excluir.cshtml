@model EnderecoModel
@{
    ViewData["Title"] = "Confirmar exclusao";
}

<div class="text-center">

    <h3>Realmente deseja excluir o endereco? (@Model.tipo)</h3>

</div>

<a class="btn btn-primary btn-block" role="button" asp-controller="Endereco" asp-action="Index">Cancelar Exclusão </a>
<a class="btn btn-danger btn-block" role="button" asp-controller="Endereco" asp-action="ExcluirEndereco" asp-route-id="@Model.id">Confirmar Exclusão </a>